
@{
    ViewBag.Title = "Index";
}

@{
    var arrayOfDocs = ViewData["arrayOfDocs"] as string[];
}

<h2>Emergency Leave Documents</h2>

<table class="table">
    <tr>
        <th>
            Files
        </th>
        <th></th>
    </tr>

    @foreach (var item in arrayOfDocs)
    {
        <tr>
            <td>
                <a href="~/Emergency_Leave_Docs/@item">@item</a>
            </td>
        </tr>
    }

</table>


