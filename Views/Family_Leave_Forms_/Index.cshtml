
@{
    ViewBag.Title = "Index";
}

@{ 
    var fileEntries = ViewData["fileEntries"] as string[];
 }

<h2>Index</h2>



<table class="table">
    <tr>
        <th>
            Files
        </th>
        <th></th>
    </tr>

    @foreach (var item in fileEntries)
    {
        <tr>
            <td>
                <a href="~/Family_Leave_Docs/@Html.DisplayFor(key => key)">@Html.DisplayFor(key => key)</a>
            </td>
        </tr>
    }

</table>


